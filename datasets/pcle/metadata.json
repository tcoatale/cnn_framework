{
    "name": "pcle",
    "classes": 2,
    "images": {
        "normal": {
            "width": 336,
            "height": 336,
            "depth": 1
        },
        "resized": {
            "width": 64,
            "height": 64,
            "depth": 1
        },
        "additional_channels": 2
    },
    "directories": {
        "raw": "data/pcle/raw", 
        "processed": "data/pcle/processed",
        "log": "log/pcle/",
        "ckpt": "log/ckpt"
    },
    "augmentations": [
        {
            "name": "Frame Extraction",       
            "input": "videos",
            "output": "frames",
            "usage": "main",
            "priority": 1, 
            "compute": false
        }, {
            "name": "Blob Extraction",
            "input": "frames",
            "output": "blob",
            "usage": "channel",
            "number": 2,
            "priority": 2, 
            "compute": false
        }, {
            "name": "Gabor Extraction",
            "input": "frames",
            "output": "gabor/output.csv",
            "usage": "none",
            "priority": 2, 
            "compute": true

        }, {
            "name": "Gabor Isomap Reduction", 
            "input": "gabor/output.csv",  
            "output": "gabor/isomap_output.csv",
            "usage": "features",
            "number": 50,
            "priority": 3, 
            "compute": true
        }
    ],
    "bytes": {
        "identifier": 60,
        "label": 1
    },
    "images_per_batch_file": 1000, 
    "set_sizes": {
      "train": 5024, 
      "test": 1385
    } 
}