{
    "name": "pcle",
    "sets": {
        "train": 4500,
        "test": 1500
    },
    "classes": 2,
    "images": {
        "normal": {
            "width": 336,
            "height": 336,
            "depth": 1
        },
        "resized": {
            "width": 64,
            "height": 64,
            "depth": 1
        },
        "additional_channels": 4
    },
    "directories": {
        "raw": "data/pcle/raw", 
        "processed": "data/pcle/processed"
    },
    "augmentations": [
{
            "name": "Blob Extraction",
            "input": "frames",
            "output": "blob",
            "usage": "channel",
            "number": 1,
            "priority": 2
        },{
            "name": "Gabor Extraction",
            "input": "frames",
            "output": "gabor/output.csv",
            "usage": "none",
            "priority": 2

        },{
            "name": "Gabor Isomap Reduction", 
            "input": "gabor/output.csv",  
            "output": "gabor/isomap_output.csv",
            "usage": "features",
            "number": 50,
            "priority": 3
        }
    ],
    "bytes": {
        "identifier": 60,
        "label": 1
    },
    "images_per_batch_file": 100
}